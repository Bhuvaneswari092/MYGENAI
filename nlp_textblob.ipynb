        "from textblob import TextBlob\n",
        "\n",
        "text = \"The quick brown fox jumps over the lazy dog\"\n",
        "\n",
        "blob = TextBlob(text)\n",
        "\n",
        "for word, pos in blob.tags:\n",
        "    print(word, pos)"
